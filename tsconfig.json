{
	"compilerOptions": {
		"noEmit": true,
		"checkJs": true,
		"strict": true,
		"allowJs": true,
		"target": "ESNext",
		"moduleResolution": "NodeNext",
		"module": "NodeNext",
		"lib": ["DOM", "DOM.Iterable", "ESNext"],
		"allowImportingTsExtensions": true,
		"forceConsistentCasingInFileNames": false,
		"skipLibCheck": false
	},
}